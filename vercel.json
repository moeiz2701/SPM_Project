{
  "builds": [
    {
      "src": "api/agent_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/agent_api.py"
    }
  ],
  "installCommand": "pip install -r requirements-vercel.txt"
}
